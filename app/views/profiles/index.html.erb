<div class="container">
<h1 class="fst-italic text-center">My Profile</h1>
<%= link_to "Create Your Profile", new_profile_path() ,class: "btn btn-primary rounded-pill w-auto h-auto fst-italic mb-2"%>

<div class="row">
  <div id="profiles">
  <% @profiles.each do |profile| %>
    <%= render profile %>
    <p>
      <%#= link_to "Show this profile", profile %>
    </p>
  <% end %>
</div>
</div>